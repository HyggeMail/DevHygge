@model List<FAQModel>
<ul class="accordion_example">
    @{var count = 1;}
    @if (Model.Count > 0 && Model != null)
    {
        
        foreach (var item in Model)
        {
            <li class="card">
                <div class="accordian-head">
                    @count. @item.Title
                </div>
                <div>
                    @Html.Raw(item.Description)
                </div>
            </li>
            count += 1;
        }
    }
    else
    {
        <li class="card">
            <div class="accordian-head">
                No Record Found
            </div>
        </li>
    }
</ul>