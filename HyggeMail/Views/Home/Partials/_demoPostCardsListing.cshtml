@model List<AddUpdateImageEditorModel>
@{
    UserDetails LOGGEDIN_USERInfo = (UserDetails)ViewBag.LOGGEDIN_USER;
}

@{
    if (Model != null && Model.Count > 0)
    {
        <div class="thumbs-sec">
            @foreach (var obj in Model)
            {
                if (LOGGEDIN_USERInfo.IsAuthenticated)
                {
                    <div class="item homePostCardClick" data-toggle="modal" data-target="#popup-card" data-href="@Url.Action("EditDemoCard", "Home", new { cid = @obj.ID, userId = @obj.UserID })" data-cardfrontpath="@obj.CardFrontPath.Replace("~",string.Empty)" data-cardbackpath="@obj.CardBackPath.Replace("~",string.Empty)">
                        <a href="javascript:void(0)">
                            <div class="bottom-cap">
                            </div>
                            <img class="" src="@obj.CardFrontPath.Replace("~", string.Empty)" alt="">
                        </a>
                    </div>
                }
                else
                {
                    <div class="item homePostCardClick" data-toggle="modal" data-target="#popup-card" data-returnurl="@Url.Action("EditDemoCard", "Home", new { cid = @obj.ID, userId = @obj.UserID })" data-cardfrontpath="@obj.CardFrontPath.Replace("~",string.Empty)" data-cardbackpath="@obj.CardBackPath.Replace("~",string.Empty)">
                        <a href="javascript:void(0)">
                            <div class="bottom-cap">
                            </div>
                            <img class="" src="@obj.CardFrontPath.Replace("~",string.Empty)" alt="">
                        </a>
                    </div>
                }
            }
        </div>
    }
    else
    {
        <div class="thumbs-sec">
            <div class="item" data-toggle="modal" data-target="#popup-card"> <a href="javascript:void(0)"> <div class="bottom-cap"> </div> <img class="" src="~/content/images/gallery-1.jpg" alt=""> </a> </div>
            <div class="item" data-toggle="modal" data-target="#popup-card"> <a href="javascript:void(0)"> <div class="bottom-cap"> </div> <img class="" src="~/content/images/gallery-2.jpg" alt=""> </a> </div>
            <div class="item" data-toggle="modal" data-target="#popup-card"> <a href="javascript:void(0)"> <div class="bottom-cap"> </div> <img class="" src="~/content/images/gallery-3.jpg" alt=""> </a> </div>
            <div class="item" data-toggle="modal" data-target="#popup-card"> <a href="javascript:void(0)"> <div class="bottom-cap"> </div> <img class="" src="~/content/images/gallery-4.jpg" alt=""> </a> </div>
            <div class="item" data-toggle="modal" data-target="#popup-card"> <a href="javascript:void(0)"> <div class="bottom-cap"> </div> <img class="" src="~/content/images/gallery-5.jpg" alt=""> </a> </div>
            <div class="item" data-toggle="modal" data-target="#popup-card"> <a href="javascript:void(0)"> <div class="bottom-cap"> </div> <img class="" src="~/content/images/gallery-6.jpg" alt=""> </a> </div>
            <div class="item" data-toggle="modal" data-target="#popup-card"> <a href="javascript:void(0)"> <div class="bottom-cap"> </div> <img class="" src="~/content/images/gallery-7.jpg" alt=""> </a> </div>
            <div class="item" data-toggle="modal" data-target="#popup-card"> <a href="javascript:void(0)"> <div class="bottom-cap"> </div> <img class="" src="~/content/images/gallery-8.jpg" alt=""> </a> </div>
        </div>
    }

}


<!-- Modal -->
<div class="modal fade modal-custom card-popup" id="popup-card" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                        <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                        <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                    </ol>

                    <!-- Wrapper for slides -->
                    <div class="carousel-inner" role="listbox">
                        <div class="item active">
                            <img src="" id="srcCardfrontpath">
                        </div>
                        <div class="item">
                            <img src="" id="srcCardbackpath">
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                @if (LOGGEDIN_USERInfo != null && LOGGEDIN_USERInfo.IsAuthenticated)
                {
                    <a href="javascript:void(0)" id="setPostCardEditHREF">
                        <span class="edit-card" title="Edit Card">
                            <i class="fa fa-files-o" aria-hidden="true"></i>
                        </span>
                    </a>
                }
                else
                {
                    <a href="javascript:;" data-dismiss="modal" data-toggle="modal" data-target="#myModal-signin">
                        <span class="edit-card" title="Edit Card">
                            <i class="fa fa-files-o" aria-hidden="true"></i>
                        </span>
                    </a>
                }
            </div>
        </div>
    </div>
</div>