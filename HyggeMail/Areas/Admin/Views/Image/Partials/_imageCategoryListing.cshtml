@model PagingResult<AdminImageCategoryListingModel>

        @if (Model.List != null && Model.List.Count() > 0)
        {
            foreach (var item in Model.List)
            {
                <tr class="odd gradeX">
                    <td>@item.CategoryName</td>
                    <td> @Convert.ToDateTime(item.AddedOn).ToString("MMM/dd/yyyy")</td>
                    <td class="center"> 
                        <a class="btn btn-primary btn-xs" href="@Url.Action("AddImageCategory", "Image", new { id = item.ID, area = "Admin" })" data-original-title="Edit" data-toggle="tooltip"><i class="fa fa-pencil"></i></a>
                        <a data-imagecat="@item.ID" class="btn btn-danger btn-xs deleteImageCategory" href="javascript:void(0);" data-original-title="Remove" data-toggle="tooltip"><i class="fa fa-times"></i></a>
                     </td>
                </tr>
            }
        }
        else
        {
            <tr class="odd gradeX">
                <td colspan="5">No Record Found</td>
            </tr>
        }

