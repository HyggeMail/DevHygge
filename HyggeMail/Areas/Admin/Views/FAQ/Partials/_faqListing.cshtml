@model PagingResult<FAQModel>


@if (Model.List != null && Model.List.Count() > 0)
{
    foreach (var item in Model.List)
    {
        <tr class="odd gradeX">
            <td>@item.Title</td>
            <td>@item.CategoryID.ToString()</td>
            <td class="center">@Convert.ToDateTime(item.AddedOn).ToString("MMM dd yyyy hh:mm tt")</td>
            <td class="center">@Convert.ToDateTime(item.UpdatedOn).ToString("MMM dd yyyy hh:mm tt")</td>
            <td class="center">
                <a class="btn btn-primary btn-xs" href="@Url.Action("AddEditFAQ", "FAQ", new { area = "Admin", id = @item.ID })" data-original-title="Edit" data-toggle="tooltip"><i class="fa fa-pencil"></i></a>
                <a data-id="@item.ID" class="btn btn-danger btn-xs deletefaq" href="javascript:void(0);" data-original-title="Remove" data-toggle="tooltip"><i class="fa fa-times"></i></a>
            </td>
        </tr>
    }
}
else
{
    <tr class="odd gradeX">
        <td colspan="5">No Record Found</td>
    </tr>
}




